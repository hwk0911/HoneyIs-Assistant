{{>layout/header}}

<div class="container">
    <!-- 분석 결과 출력 -->
    <table class="table table-hover table-dark" id="stockListTable">
        <thead>
        <tr>
            <th width="30%">CLIENT NAME</th>
            <th width="30%">PRODUCT NAME</th>
            <th width="15%">COLOR</th>
            <th width="15%">SIZE</th>
            <th width="10%">AMOUNT</th>
        </tr>
        </thead>
    <tbody>
        <tr>
            <td>{{key}}</td>
            <td>{{productName}}</td>
            <td>{{color}}</td>
            <td>{{size}}</td>
            <td>{{amount}}</td>
            <td style="display:none">{{id}}</td>
        </tr>
</tbody>
</table>

{{>layout/footer}}